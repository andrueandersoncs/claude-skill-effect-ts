{
	"name": "effect-ts-examples",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"check": "tsc --noEmit",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"detect": "bun run detectors/runner.ts",
		"detect:all": "bun run detectors/runner.ts -s info",
		"detect:errors": "bun run detectors/runner.ts -s error --no-potential",
		"detect:json": "bun run detectors/runner.ts --json"
	},
	"dependencies": {
		"@effect/schema": "^0.75.5",
		"effect": "^3.14.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@effect/vitest": "^0.18.0",
		"@types/node": "^25.1.0",
		"typescript": "^5.7.0"
	}
}
